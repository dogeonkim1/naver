package day1211;

public class Book43 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//43페이지 진법변환 예제
		int a=0b1011;
		int b=0B10100;
		int c=013;
		int d=0206;
		int e=12;
		int f=365;
		
		
		System.out.println("a="+a);
		System.out.println("b="+b);
		System.out.println("c="+c);
		System.out.println("d="+d);
		System.out.println("e="+e);
		System.out.println("f="+f);

	}

}
