package day1211;

public class Ex7DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="Candy";
		int age=32;
		float height=163.8f;
		double blood='b';
		/*
		 * 이름 : Candy님
		 * 나이 : 32세
		 * 키 : 163.8cm
		 * 몸무게 :56.7kg
		 * 혈액형 :b형
		 */
		//printf를 이용하여 위와같이 출력
		System.out.printf("이름:%s\n",name+"님");
		System.out.printf("나이:%s\n",age+"세");

	}

}
